        ]
  },
  "class": "CommonGateway\\CoreBundle\\ActionHandler\\RequestProxyHandler",
  "configuration": {
    "source": "https://zds.vng.nl/endpoints/zgw.zrc.source.json"
  },
  "isLockable": false,
  "isEnabled": true
