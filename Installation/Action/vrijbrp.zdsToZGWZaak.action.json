  "$id": "https://vrijbrp.nl/action/vrijbrp.zdsToZRC.action.json",
  "$schema": "https://docs.commongateway.nl/schemas/Action.schema.json",
  "version": "0.0.1",
  "listens": [
    "zds.inbound"
  ],
  "conditions": {
        "==": [
          {
            "var": "body.SOAP-ENV:Body.ns2:zakLv01.ns2:stuurgegevens.ns1:entiteittype"
